.logo {
  width: 48px;
  height: 26px;
  color: #ffffff;
  font-size: 36px;
  font-weight: 800;
  line-height: 20px;
  cursor: pointer;
  transition: 0.76s;
  text-decoration: none;
  -webkit-text-stroke-width: 1px;
  -webkit-text-stroke-color: black;
  &:hover {
    /* text-shadow: white 0 0 4px; */
    /* opacity: .5; */
    transition: 0.76s;
    color: #ff9a00;
  }
}
